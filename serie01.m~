x = [pi / 4, pi / 2, 3 * pi / 4, 2 * pi];
xvec = 0:0.5:3.5;
yvec = cos(xvec);
y = LagrangeInterpolation(x, xvec, yvec);

disp(['sampling points: [' num2str(xvec(:).') ']'])